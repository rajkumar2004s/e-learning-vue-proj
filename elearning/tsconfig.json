{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": false,
    "skipLibCheck": true,
    "noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": [
    "nuxt.config.ts",
    "app/**/*.ts",
    "app/**/*.vue",
    "app/**/*.d.ts",
    "app/plugins/**/*.ts",
    "app/stores/**/*.ts"
  ]
}
